{
  "name": "ld-react-components",
  "version": "1.0.81",
  "description": "Semantic component helpers to support LaunchDarkly in your react app.",
  "main": "dist/index.js",
  "author": {
    "name": "Dave Bergschneider",
    "email": "dave@cmscode.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/LD-React-Components"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "scripts": {
    "start": "start-storybook -p 6010",
    "prepare": "npm run snyk-protect && yarn build",
    "clean": "rimraf ./dist ./coverage ./docs ./test-results",
    "test": "yarn add --dev jest-junit && yarn build && jest --coverage --reporters=default --reporters=jest-junit && mkdir -p test-reports/jest && mv junit.xml test-reports/jest/results.xml",
    "lint": "eslint src/",
    "build": "babel src/lib -d dist --copy-files && tsc",
    "build:watch": "babel src/lib -w -d lib --copy-files",
    "docs:build": "build-storybook -o docs",
    "docs:config": "mkdirp ./docs/.circleci/ && cp .storybook/config.yml docs/.circleci/ && cp -R .storybook/old-docs docs/",
    "docs:publish": "push-dir --cleanup --dir=docs --branch=gh-pages --force",
    "release": "auto shipit",
    "snyk-protect": "snyk protect"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "launch-darkly",
    "feature",
    "flags",
    "react",
    "open source"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storysource": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.26",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.10",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/storybook__react": "^5.2.1",
    "all-contributors-cli": "^6.14.2",
    "auto": "^9.34.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "~14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "~4.0.1",
    "expect": "^26.0.1",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^12.0.0",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "push-dir": "^0.4.1",
    "react": "^16.10.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.12.21",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "hash.js": "~1.1.7",
    "ldclient-js": "^2.10.2",
    "prop-types": "^15.7.2",
    "snyk": "^1.518.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "snyk": true
}
