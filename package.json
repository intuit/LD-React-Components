{
  "name": "ld-react-components",
  "version": "1.0.1",
  "description": "Semantic component helpers to support LaunchDarkly in your react app.",
  "main": "dist/index.js",
  "author": {
    "name": "Dave Bergschneider",
    "email": "dave@cmscode.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/LD-React-Components"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "scripts": {
    "prepare": "yarn build",
    "clean": "rimraf ./dist ./coverage",
    "test": "jest --coverage",
    "lint": "eslint --ext .js src/",
    "build": "babel src/lib -d dist --copy-files",
    "build:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --config src/docs/webpack.config.live.babel.js",
    "docs:build": "NODE_ENV=production webpack --config src/docs/webpack.config.live.babel.js",
    "docs:publish": "push-dir --dir=src/docs/dist --branch=gh-pages --cleanup --verbose",
    "dev": "yarn docs",
    "release": "auto shipit"
  },
  "keywords": [
    "launch-darkly",
    "feature",
    "flags",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "all-contributors-cli": "^6.9.1",
    "auto": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "~6.4.0",
    "eslint-config-standard": "~14.1.0",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-node": "~10.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.14.3",
    "eslint-plugin-standard": "~4.0.1",
    "expect": "^24.9.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "push-dir": "^0.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.12.13",
    "react-test-renderer": "^16.9.0",
    "sinon": "^7.4.2",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "hash.js": "~1.1.7",
    "ldclient-js": "^2.10.2",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
