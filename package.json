    "react": "^16.10.1",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.10.1",
    "sinon": "^7.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^5.3.18",
    "hash.js": "~1.1.7",
    "ldclient-js": "^2.10.2",
    "prop-types": "^15.7.2",
    "snyk": "^1.323.0"
  },
  "peerDependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  },
  "snyk": true
}
